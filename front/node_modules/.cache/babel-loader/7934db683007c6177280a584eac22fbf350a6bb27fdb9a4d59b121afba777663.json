ntext({}).then((response) => {\n      //     this.posts = response.data; // 将获取到的数据赋值给组件的 data 属性\n      //   }).catch((error) => {\n      //     console.error(error);\n      //   });\n    }\n  }\n};","map":{"version":3,"names":["Comment","personpost","name","components","data","currentUser","posts","id","title","content","comments","text","newCommentText","showDialog","mounted","fetchPosts","methods","then","response","catch","error","console","addComment","post","newComment","Date","now","push","addContext","e","log","target","value"],"sources":["/Users/laken/Documents/玉山銀行/front/src/views/MainView.vue"],"sourcesContent":["<template>\n  <div class=\"home\">\n    <div class=\"user-profile\">\n      <router-link to=\"/login\">登出</router-link> |\n      <span>{{ currentUser }}</span>\n    </div>\n    <div class=\"posts\">\n      <div class=\"post\" v-for=\"post in posts\" :key=\"post.id\">\n        <div style=\"display: flex;justify-content: space-between;\">\n          <div></div>\n          <h2>{{ post.title }}</h2>\n          <i class=\"fas fa-times\" @click=\"()=>console.log(123)\"></i>\n        </div>\n        <p>{{ post.content }}</p>\n        <comment :initial-comments=\"post.comments\"></comment>\n      </div>\n    </div>\n    <form @submit.prevent=\"addContext\">\n      <div class=\"form-group\">\n        <label for=\"title\"> Title : </label>\n        <input type=\"text\" id=\"title\" v-model=\"username\" required>\n        <button class=\"button\" type=\"submit\">送出</button>\n      </div>\n      <div class=\"form-group\">\n        <label for=\"context\">Context:</label>\n        <input type=\"text\" id=\"context\" v-model=\"password\" required>\n      </div>\n    </form>\n  </div>\n</template>\n\n<script>\nimport Comment from \"@/components/CommentSection.vue\";\nimport { personpost } from \"@/api.js\"\nimport '@fortawesome/fontawesome-free/css/all.css'; // 引入Font Awesome的CSS样式\nexport default {\n  name: \"HomePage\",\n  components: {\n    Comment,\n  },\n 